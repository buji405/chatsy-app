{
    "defaultSeverity": "error",
    "extends": ["tslint:recommended"],
    "jsRules": {},
    "rules": {
      "arrow-parens": false,
      "ban-types": false,
      "member-access": false,
      "max-classes-per-file": false,
      "semicolon": [true, "always", "ignore-bound-class-methods"],
      "trailing-comma": false,
      "interface-name": [true, "never-prefix"],
      "max-line-length": false
    },
    "rulesDirectory": []
  }